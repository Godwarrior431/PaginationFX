<VBox fx:id="filterTableViewVBox" minHeight="350.0" prefHeight="437.0" prefWidth="635.0" style="-fx-border-color: #ffffff; -fx-border-radius: 9; -fx-background-radius: 9; -fx-border-radius: 9;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.filter.FilterTableViewController">
